	issn = {09204105},
	journal = {Journal of Petroleum Science and Engineering},
	keywords = {Artificial Neural Network,Chebyshev polynomials,Fracture parameters,Naturally Fractured Reservoir,Stehfest algorithm,Well testing,arti fi cial neural,network},
	pages = {585--594},
	title = {{Artificial Neural Network (ANN) to estimate reservoir parameters in Naturally Fractured Reservoirs using well test data}},
