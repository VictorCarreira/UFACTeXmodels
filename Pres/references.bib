@phdthesis{Carvalho2014,
	abstract = {A melhor forma de conhecer uma forma{\c{c}}{\~{a}}o em subsuperf{\'{i}}cie e diminuir as incertezas em rela{\c{c}}{\~{a}}o {\`{a}}s suas caracter{\'{i}}sticas {\'{e}} fazer uso de m{\'{e}}todos que forne{\c{c}}am grande quantidade de informa{\c{c}}{\~{a}}o. A perfilagem de po{\c{c}}os, corriqueira e imprescind{\'{i}}vel, normalmente feita no in{\'{i}}cio dos estudos de um po{\c{c}}o, aliada a realiza{\c{c}}{\~{a}}o de testes de forma{\c{c}}{\~{a}}o, garante uma boa apresenta{\c{c}}{\~{a}}o das forma{\c{c}}{\~{o}}es. O presente trabalho se baseia no uso das medidas feitas neste procedimento, os perfis geof{\'{i}}sicos (logs), acrescidos de medidas de laborat{\'{o}}rio para caracteriza{\c{c}}{\~{a}}o petrof{\'{i}}sica do Campo de Namorado na Bacia de Campos. A primeira parte deste trabalho consiste em fundamenta{\c{c}}{\~{a}}o te{\'{o}}rica para pleno entendimento das t{\'{e}}cnicas da perfilagem de po{\c{c}}o. Ressaltando as particularidades do Campo de Namorado, como a forma{\c{c}}{\~{a}}o do reservat{\'{o}}rio relacionada {\`{a}} deposi{\c{c}}{\~{a}}o de leques turbid{\'{i}}ticos e presen{\c{c}}a de arenito arcoseano, no in{\'{i}}cio desta disserta{\c{c}}{\~{a}}o encontra- se uma breve descri{\c{c}}{\~{a}}o da geologia regional. Na sequ{\^{e}}ncia do seu conte{\'{u}}do, est{\~{a}}o defini{\c{c}}{\~{o}}es de propriedades petrof{\'{i}}sicas, sendo estas: porosidade, permeabilidade, resistividade, volume de argila e satura{\c{c}}{\~{a}}o de {\'{a}}gua. Al{\'{e}}m destas defini{\c{c}}{\~{o}}es b{\'{a}}sicas, est{\'{a}} contida tamb{\'{e}}m uma explana{\c{c}}{\~{a}}o sobre a resposta das ferramentas diante das forma{\c{c}}{\~{o}}es, a leitura e interpreta{\c{c}}{\~{a}}o dos logs utilizados: c{\'{a}}liper, raios gama, resistividade, densidade, porosidade neutr{\^{o}}nica e s{\^{o}}nico. A continua{\c{c}}{\~{a}}o do trabalho incide na aplica{\c{c}}{\~{a}}o das t{\'{e}}cnicas de perfilagem nos dados reais do Campo de Namorado, com uso do software Interactive Petrophysics{\textregistered}. Ap{\'{o}}s an{\'{a}}lise de crossplots para interpreta{\c{c}}{\~{a}}o qualitativa de zonas previamente delimitadas, ser{\~{a}}o feitos ent{\~{a}}o os c{\'{a}}lculos das propriedades petrof{\'{i}}sicas citadas acima com base nos dados extra{\'{i}}dos dos logs de po{\c{c}}o. Em seguida os dados obtidos ser{\~{a}}o confrontados com valores reais de amostras dos laborat{\'{o}}rios da Petrobras. O fim deste trabalho est{\'{a}} no ponto em que for poss{\'{i}}vel discernir perfeitamente os reservat{\'{o}}rios e suas delimita{\c{c}}{\~{o}}es, assim como os outros elementos do sistema petrol{\'{i}}fero e suas caracter{\'{i}}sticas, como litologia e qualidade.},
	author = {de Carvalho, Patr{\'{i}}cia Bay{\~{a}}o},
	file = {:C$\backslash$:/Users/ibsen/Desktop/Monografia{\_}UFF/Refer{\^{e}}ncias/Refer{\^{e}}ncias/Patricia{\_}Bayao{\_}de{\_}Carvalho{\_}2013.pdf:pdf},
	keywords = {Campo de Namorado,Perfilagem geof{\'{i}}sica,Propriedades petrof{\'{i}}sicas,Reservat{\'{o}}rio},
	title = {{Caracteriza{\c{c}}{\~{a}}o Petrof{\'{i}}sica do Campo de Namorado a partir de Perfis de po{\c{c}}o}},
