@article{Poulton2002,
	abstract = {The sophisticated algorithms we use to process, analyze, and interpret$\backslash$ngeophysical data automate tasks we used to do by hand, transform data$\backslash$ninto domains where patterns are more obvious, and allow us to calculate$\backslash$nquantities where we used to rely on intuition or back-of-envelope$\backslash$nestimates. But, the crux of the exploration problem is still$\backslash$ninterpretation-associating abstract patterns with geologic formations of$\backslash$neconomic value. Artificial neural networks are able to couple the speed$\backslash$nand efficiency of the computer with the pattern recognition and$\backslash$nassociation capabilities of the brain to aid the exploration process.$\backslash$nThe key concept to understand in the application of neural network$\backslash$ntechnology is that they should not be used as an artificial intelligence$\backslash$ntool to replace a human interpreter; rather, neural networks are an$\backslash$nintelligence amplification toolkit that allows the interpreter to focus$\backslash$non the important information.$\backslash$nMore than 102 neural network papers have been published by SEG since$\backslash$n1988, and more than 550 neural network papers pertaining to any aspect$\backslash$nof geophysics were published in that same time period. Neural network$\backslash$napplications in exploration geophysics can generally be divided into two$\backslash$neras. The focus through 1994 was largely on learning what neural$\backslash$nnetworks could do with different data sets, and how to prepare data for$\backslash$nthem and analyze the results. Networks were usually trained with$\backslash$nsynthetic data and then tested with field data. The second era, from$\backslash$n1995 to the present, has focused on some specific application areas such$\backslash$nas reservoir characterization. The current emphasis is to integrate$\backslash$nneural networks within a comprehensive interpretation scheme instead of$\backslash$nas a stand-alone application. Neural network technology has helped us$\backslash$nturn data into information by allowing us to find nontrivial$\backslash$ncorrelations between geophysical data and petrophysical properties;$\backslash$nrelate detailed changes in wavelet morphology to small-scale changes in$\backslash$nlithology; and find features in the wavelets that allow us to locate$\backslash$nfirst breaks, track horizons, identify gas chimneys, or trace faults;$\backslash$nand attenuate multiples. As the science and engineering of data$\backslash$nacquisition progresses, neural networks will play an increasingly vital$\backslash$nrole in helping us find relevant information in the vast streams of data$\backslash$nunder the constraints of lower costs, less time, and fewer people.},
	author = {Poulton, Mary M.},
	doi = {10.1190/1.1484539},
	file = {:home/vimana/Dropbox/Doutorado/Redes Neurais/Poulton2002.pdf:pdf},
	issn = {0016-8033},
	journal = {Geophysics},
	number = {3},
	pages = {979},
	title = {{Neural networks as an intelligence amplification tool: A review of applications}},
	volume = {67},
	year = {2002}
}

@article{Perol2018,
	abstract = {The recent evolution of induced seismicity in Central United States calls for exhaustive catalogs to improve seismic hazard assessment. Over the last decades, the volume of seismic data has increased exponentially, creating a need for efficient algorithms to reliably detect and locate earthquakes. Today's most elaborate methods scan through the plethora of continuous seismic records, searching for repeating seismic signals. In this work, we leverage the recent advances in artificial intelligence and present ConvNetQuake, a highly scalable convolutional neural network for earthquake detection and location from a single waveform. We apply our technique to study the induced seismicity in Oklahoma (USA). We detect 20 times more earthquakes than previously cataloged by the Oklahoma Geological Survey. Our algorithm is orders of magnitude faster than established methods.},
	author = {Perol, Thibaut and Gharbi, Micha{\"{e}}l and Denolle, Marine},
	doi = {10.1126/sciadv.1700578},
	file = {:home/carreira/Documentos/Revis{\~{a}}o bibliogr{\'{a}}fica/PerolT2018.pdf:pdf},
	issn = {23752548},
	journal = {Science Advances},
	number = {2},
	pages = {2--10},
	title = {{Convolutional neural network for earthquake detection and location}},
	volume = {4},
	year = {2018}
}
@article{Peters2019,
	abstract = {Neural-networks have seen a surge of interest for the interpretation of seismic images during the last few years. Network-based learning methods can provide fast and accurate automatic interpretation, provided there are sufficiently many training labels. We provide an introduction to the field aimed at geophysicists that are familiar with the framework of forward modeling and inversion. We explain the similarities and differences between deep networks to other geophysical inverse problems and show their utility in solving problems such as lithology interpolation between wells, horizon tracking and segmentation of seismic images. The benefits of our approach are demonstrated on field data from the Sea of Ireland and the North Sea.},
	archivePrefix = {arXiv},
	arxivId = {1903.11215},
