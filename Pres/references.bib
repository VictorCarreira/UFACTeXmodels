@article{Carneiro2012,
	abstract = {A self-organizing map (SOM) approach has been used to provide an integrated spatial analysis and classification of airborne geophysical data collected over the Brazilian Amazon. Magnetic and gamma ray spectrometric data were used to extract geophysical signatures related to the spatial distribution of rock types and to produce a geologic map over the prospective Anapu-Tuer{\^{e}} region. Particular emphasis was given to discriminating and identifying rock types, and the processes related to gold mineralization, which are known to occur in the Anapu-Tuer{\^{e}} region. SOM was able to identify and map distinctive geophysical signatures related to the various geologic units identified on the published geologic map. Furthermore, SOM was able to identify and enhance very subtle signatures derived jointly from the magnetic and gamma ray spectrometric data that could be related to geologic processes present in the area. These results demonstrate the effectiveness of using SOM as a tool for geophysical data analysis and for semiautomated mapping in regions such as the Amazon. {\textcopyright} 2012 Society of Exploration Geophysicists.},
	author = {Carneiro, Cleyton de Carvalho and Fraser, Stephen James and Cr{\'{o}}sta, Alvaro Penteado and Silva, Adalene Moreira and Barros, Carlos Eduardo de Mesquita},
	doi = {10.1190/geo2011-0302.1},
	file = {:home/carreira/Documentos/Revis{\~{a}}o bibliogr{\'{a}}fica/Carneiro2012.pdf:pdf},
	issn = {0016-8033},
	journal = {Geophysics},
	number = {4},
	pages = {K17--K24},
	title = {{Semiautomated geologic mapping using self-organizing maps and airborne geophysics in the Brazilian Amazon}},
	volume = {77},
	year = {2012}
}

@article{Carreira2013,
	abstract = {This article presents a geologic-geophysical model along a 320 km NW – SE profile at the central region of Paran{\'{a}} basin, based primarily on a 2D gravity model, in complement to collocated magnetotelluric (MT) studies that are being carried out by the Brazilian National Agency of Petroleum, Natural Gas and Biofuels - ANP. The study of large structures within the basement underneath a sedimentary basin is of vital importance for the understanding of its tectonic-stratigraphic evolution. The Paran{\'{a}} basin is a shallow basin with a depocenter reaching approximately 7, 000m of estimated depth. Two large regional structures are remarkable in the study area: the Ponta Grossa Arch, a NW-SE direction feature; and the gravity high of Paranapanema, an elongated NW-SE positive anomaly. The region also includes large lava flows represented by basalt rocks of the Serra Geral Formation. Intrusion of dikes and sills of diabase, dating mainly from the Cretaceous, are equally striking in the region. These intrusions took advantage of certain bygone structural lineament groups to settle. The Bouguer surface of the study area was represented by a 4th degree polynomial, to estimate the depth of Moho. The proposed final gravimetric model includes the regional topography and presents good fit, with rms error of 1.7 mGal. Its shallowest part was obtained by taking into ac- count data from six existing stratigraphic wells in the area and also integrating 1D electrical resistivity model links from new magnetotelluric measured data in regional profile. The deepest structures of the model were connected to studies of deep seismic refraction, receptor function analysis and surface waves‘ dispersion, in particular the depth of the Moho. Tests with synthetic MT models support the proposed geological - geophysical model which present some notable characteristics: the crust – mantle interface is very smooth and oscillates around 42 km deep. The upper crust was represented by two blocks that correspond, in the northwestern portion, to the Precambrian continental crust and Paranapanema and, in the southeastern portion, to a mobile belt, also Precambrian, called Rio Apia{\'{i}}, characterized by positive residual gravity anomaly, in accordance with the tectonic context of the region already described in previous studies. The basin-basement limit, derived from the MT model, revealed a fractured surface with an average depth of 5.5 km. The sedimentary portion can be differentiated in its established super-sequences, but does not resolve the problematic of dikes and sills for the studied profile},
	annote = {Artigo do mestrado},
	archivePrefix = {arXiv},
	arxivId = {arXiv:1011.1669v3},
	author = {Carreira, Victor Ribeiro and {La Terra}, Emanuele Francesco and Fontes, Sergio Luis},
	doi = {10.1017/CBO9781107415324.004},
	eprint = {arXiv:1011.1669v3},
	file = {:home/vimana/Dropbox/Mestrado/Artigo/APPGEO4437.pdf:pdf},
	isbn = {9788578110796},
	issn = {1098-6596},
	journal = {Journal of Applied Geophysics},
	keywords = {icle},
	number = {9},
	pages = {1689--1699},
	pmid = {25246403},
	title = {{Density model for the central region of the Paran{\'{a}} basin, south portion of Brazil.}},
	volume = {53},
	year = {2013}
}

@article{Chaudhary2014,
	abstract = {The Self-Organizing Map (SOM) has applications like dimension reduction, data clustering, image analysis, and many others. In conventional SOM, the weights of the winner and its neighboring neurons are updated regardless of their distance from the input vector. In the proposed SOM, the farthest and nearest neurons from among the 1-neighborhood of the winner neuron, and also the winning frequency of each neuron are found out and taken into account while updating the weight. This new SOM is applied to various input data sets and the learning performance is evaluated using three standard measurements. It is confirmed that modified SOM obtained a far better result and better effective mapping as compared to the conventional SOM, which reflects the input data distribution.},
	author = {Chaudhary, Vikas and Bhatia, R. S. and Ahlawat, Anil K.},
	doi = {10.1016/j.aej.2014.09.007},
	file = {:home/carreira/Documentos/Revis{\~{a}}o bibliogr{\'{a}}fica/Chaudhary2014.pdf:pdf},
	issn = {11100168},
	journal = {Alexandria Engineering Journal},
	keywords = {(SOM),Farthest neuron,Map,Nearest neuron,Neighborhood neurons,Self-Organizing,Winning frequency},
	number = {4},
	pages = {827--831},
	publisher = {Faculty of Engineering, Alexandria University},
	title = {{A novel Self-Organizing Map (SOM) learning algorithm with nearest and farthest neurons}},
	url = {http://dx.doi.org/10.1016/j.aej.2014.09.007},
	volume = {53},
	year = {2014}
}


