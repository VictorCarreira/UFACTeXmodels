	journal = {Brazilian Journal of Geology},
	keywords = {Parana Basin,Parnaiba Basin,Solimoes Basin},
	number = {4},
	pages = {62--85},
	pmid = {11196637},
	title = {{An outline of the geology and petroleum systems of the Paleozoic interior basins of South America}},
	volume = {75},
	year = {2008}
}

