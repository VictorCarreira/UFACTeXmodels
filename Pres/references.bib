	isbn = {1365-246X},
	issn = {0956540X},
	journal = {Geophysical Journal International},
	keywords = {Artificial intelligence,Borehole geophysics,Drill cores,Ocean drilling,Sediments,Statistical methods},
	number = {2},
	pages = {477--491},
	title = {{Inferring the lithology of borehole rocks by applying neural network classifiers to downhole logs: An example from the Ocean Drilling Program}},
	volume = {136},
	year = {1999}
}
