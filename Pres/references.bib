	issn = {09269851},
	journal = {Journal of Applied Geophysics},
	keywords = {Cross validation technique,Crystalline rocks,Geophysical statistics,Lithology,Neural networks,Well log interpretation},
	pages = {37--46},
	publisher = {Elsevier B.V.},
	title = {{Capability of self-organizing map neural network in geophysical log data classification: Case study from the CCSD-MH}},
