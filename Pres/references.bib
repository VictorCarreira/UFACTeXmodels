@article{Simard2000,
	abstract = {The objective of this paper is to study the use of a decision tree$\backslash$nclassifier and multiscale texture measures to extract thematic$\backslash$ninformation on the tropical vegetation cover from the Global Rain Forest$\backslash$nMapping (GRFM) JERS-1 SAR mosaics. The authors focus their study on a$\backslash$ncoastal region of Gabon, which has a variety of land cover types common$\backslash$nto most tropical regions. A decision tree classifier does not assume a$\backslash$nparticular probability density distribution of the input data, and is$\backslash$nthus well adapted for SAR image classification. A total of seven$\backslash$nfeatures, including wavelet-based multiscale texture measures (at scales$\backslash$nof 200, 400, and 800 m) and multiscale multitemporal amplitude data (two$\backslash$ndates at scales 100 and 400 m), are used to discriminate the land cover$\backslash$nclasses of interest. Among these layers, the best features for$\backslash$nseparating classes are found by constructing exploratory decision trees$\backslash$nfrom various feature combinations. The decision tree structure stability$\backslash$nis then investigated by interchanging the role of the training samples$\backslash$nfor decision tree growth and testing. They show that the construction of$\backslash$nexploratory decision trees can improve the classification results. The$\backslash$nanalysis also proves that the radar backscatter amplitude is important$\backslash$nfor separating basic land cover categories such as savannas, forests,$\backslash$nand flooded vegetation. Texture is found to be useful for refining$\backslash$nflooded vegetation classes. Temporal information from SAR images of two$\backslash$ndifferent dates is explicitly used in the decision tree structure to$\backslash$nidentify swamps and temporarily flooded vegetation},
	author = {Simard, Marc and Saatchi, Sasan S. and {De Grandi}, Gianfranco},
	doi = {10.1109/36.868888},
	file = {:home/vimana/Documentos/simard2000.pdf:pdf},
	isbn = {0196-2892},
	issn = {01962892},
	journal = {IEEE Transactions on Geoscience and Remote Sensing},
	keywords = {Decision tree,Forest,Multiscale,Synthetic aperture radar (sar)},
	number = {5 I},
	pages = {2310--2321},
	title = {{The use of decision tree and multiscale texture for classification of JERS-1 SAR data over tropical forest}},
	volume = {38},
	year = {2000}
}
@article{Chen2006,
	abstract = {This study integrates log-derived empirical formulas and the concept of the committee machine to develop an improved model for predicting permeability. A set of three empirical formulas, such as the Wyllie-Rose, Coates-Dumanoir, and porosity models to correlate reservoir well-logging information with measured core permeability, are used as expert members in a committee machine. A committee machine, a new type of neural network, has a parallel architecture that fuses knowledge by combining the individual outputs of its experts to arrive at an overall output. In this study, an ensemble-based committee machine with empirical formulas (CMEF) is used. This machine combines three individual formulas, each of which performs the same evaluation task. The overall output of each ensemble member is then computed according to the coefficients (weights) of the ensemble averaging method that reflects the contribution of each formula. The optimal combination of weights for prediction is also investigated using a genetic algorithm. We illustrate the method using a case study. Eighty-two data sets composed of well log data and core data were clustered into 41 training sets to construct the model and 41 testing sets to validate the model's predictive ability. A comparison of prediction results from the CMEF model and from three individual empirical formulas showed that the proposed CMEF model for permeability prediction provided the best generalization and performance for validation. This indicated that the CMEF model was more accurate than any one of the individual empirical formulas performing alone. ?? 2005 Elsevier Ltd. All rights reserved.},
	author = {Chen, Chang Hsu and Lin, Zsay Shing},
	doi = {10.1016/j.cageo.2005.08.003},
	file = {:home/vimana/Documentos/chen2006.pdf:pdf},
	issn = {00983004},
	journal = {Computers and Geosciences},
	keywords = {Committee machine,Empirical formulas,Genetic algorithm,Permeability,Well log},
	number = {4},
	pages = {485--496},
	title = {{A committee machine with empirical formulas for permeability prediction}},
	volume = {32},
	year = {2006}
}
